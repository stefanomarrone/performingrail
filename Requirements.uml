  <Requirements:Requirement xmi:id="_wcXwkJuVEey1k9qWv3EIDg" base_NamedElement="_wcLjUJuVEey1k9qWv3EIDg" base_Class="_wcLjUJuVEey1k9qWv3EIDg"/>
  <Requirements:Requirement xmi:id="_x4VYQJuVEey1k9qWv3EIDg" base_NamedElement="_x4SU8JuVEey1k9qWv3EIDg" base_Class="_x4SU8JuVEey1k9qWv3EIDg"/>
  <Requirements:Requirement xmi:id="_ilwBoE97Eeyk7Y76mp5TBg" base_NamedElement="_ilm3sE97Eeyk7Y76mp5TBg" id="REQ-LossComms" text="Following Loss of Communications, the area of track in front of the train is considered to have Track Status Unknown, as the train may be anywhere between the last Confirmed Rear End of the train and the most recent End of Authority." base_Class="_ilm3sE97Eeyk7Y76mp5TBg"/>
  <Requirements:Requirement xmi:id="_4vuoUE-uEey239YQVA8-rw" base_NamedElement="_4vsMEE-uEey239YQVA8-rw" id="REQ-RecoveryMgmt" text="Fast recovery of the railway state after a train has lost communication is key to ensuring availability of the ETCS Level 3 Moving Block system." base_Class="_4vsMEE-uEey239YQVA8-rw"/>
  <Requirements:Requirement xmi:id="_8n5EsEFgEeywTZC05_SuTw" base_NamedElement="_8nyXAEFgEeywTZC05_SuTw" id="REQ_Sweeping" text="Sweeping is that state of the system where a train in OS or SR mode, moves into an  unknown track status area in order to clear it in a safe manner." base_Class="_8nyXAEFgEeywTZC05_SuTw"/>
  <Requirements:Requirement xmi:id="_A7heYHz8EeyMX6xfbLFcdw" base_NamedElement="_A7aJoHz8EeyMX6xfbLFcdw" id="REQ-PTS" text="Points will be controlled by the L3 Trackside in the normal way, for example by the setting of Routes.&#xD;&#xA;" base_Class="_A7aJoHz8EeyMX6xfbLFcdw"/>
  <Requirements:Requirement xmi:id="_FJxPYFEpEeypHNCzQhPY5A" base_NamedElement="_FJsW4FEpEeypHNCzQhPY5A" id="REQ-MovSR" base_Class="_FJsW4FEpEeypHNCzQhPY5A"/>
  <Requirements:Requirement xmi:id="_zitQsD1hEeyAv_UCn311nQ" base_NamedElement="_zihDcD1hEeyAv_UCn311nQ" id="REQ-LossTI" text="The L3 system shall manage the loss of train integrity in a safe manner&#xD;&#xA;" base_Class="_zihDcD1hEeyAv_UCn311nQ"/>
  <Requirements:Requirement xmi:id="_KlICUEFmEeyfxc1FgnjdUg" base_NamedElement="_Kk7OAEFmEeyfxc1FgnjdUg" id="REQ-MA" text="On-Sight movement enables the trains to enter into an occupied line for the purposes of joining or to check for infrastructure defects." base_Class="_Kk7OAEFmEeyfxc1FgnjdUg"/>
  <Requirements:Requirement xmi:id="_EZdcQIAaEeyReZuR5F228A" base_NamedElement="_EZLvcIAaEeyReZuR5F228A" id="REQ-TrackInit" text="In order to provide a safe initialisation, the L3 Trackside has to start in the most restrictive state." base_Class="_EZLvcIAaEeyReZuR5F228A"/>
  <Requirements:Requirement xmi:id="_sSFdwFOREeymBZkA5Ubaxw" base_NamedElement="_sR4CYFOREeymBZkA5Ubaxw" id="REQ-Reserved" base_Class="_sR4CYFOREeymBZkA5Ubaxw"/>
  <Requirements:Requirement xmi:id="_oxocgJzUEeyDJZoUUqZ4Zg" base_NamedElement="_owaUgJzUEeyDJZoUUqZ4Zg" base_Class="_owaUgJzUEeyDJZoUUqZ4Zg"/>
  <Requirements:Requirement xmi:id="_ZaMysJ2IEeyInc_PVJ5xIQ" base_NamedElement="_ZaBMgJ2IEeyInc_PVJ5xIQ" base_Class="_ZaBMgJ2IEeyInc_PVJ5xIQ"/>
  <Requirements:Requirement xmi:id="_iVQCIJ2IEeyInc_PVJ5xIQ" base_NamedElement="_iVNl4J2IEeyInc_PVJ5xIQ" base_Class="_iVNl4J2IEeyInc_PVJ5xIQ"/>
  <Requirements:Requirement xmi:id="_5_Zb4J2LEeyInc_PVJ5xIQ" base_NamedElement="_5_WYkJ2LEeyInc_PVJ5xIQ" id="REQ-TrainLoc-7" text="The L3 Trackside shall be able to request the ETCS On-board to report train position as soon as its Confirmed Rear End is in advance of a specified location." base_Class="_5_WYkJ2LEeyInc_PVJ5xIQ"/>
  <Requirements:Requirement xmi:id="_L9iccJ2MEeyInc_PVJ5xIQ" base_NamedElement="_L9eLAJ2MEeyInc_PVJ5xIQ" id="REQ-TrainLoc-10" text="The L3 Trackside shall ignore Train Position Reports from Non-Leading or Sleeping engines to establish Train Location." base_Class="_L9eLAJ2MEeyInc_PVJ5xIQ"/>
  <Requirements:Requirement xmi:id="_S9htYJ2MEeyInc_PVJ5xIQ" base_NamedElement="_S9cN0J2MEeyInc_PVJ5xIQ" id="REQ-TrainLoc-13" text="When after Start of Mission receiving a new set Validated Train Data from a train where the train length is longer than the train length which is stored for this train, the L3 Trackside shall adjust the extent of the Track Status Area associated with the location of this train accordingly." base_Class="_S9cN0J2MEeyInc_PVJ5xIQ"/>
  <Requirements:Requirement xmi:id="_W44IoJ2MEeyInc_PVJ5xIQ" base_NamedElement="_W41sYJ2MEeyInc_PVJ5xIQ" id="REQ-TrainLoc-14" text="When receiving new Validated Train Data from a train where the train length is longer than the train length which is stored for this train, and the new location established for this train extends into a previously Clear area of track by more than the L3 Margin, the L3 Trackside shall alert the TMS to the situation." base_Class="_W41sYJ2MEeyInc_PVJ5xIQ"/>
  <Requirements:Requirement xmi:id="_i8frgJ2MEeyInc_PVJ5xIQ" base_NamedElement="_i8dPQJ2MEeyInc_PVJ5xIQ" id="REQ-TrackStatus-10" text="On request from the TMS, the L3 Trackside shall remove or reduce Unknown Track Status Areas." base_Class="_i8dPQJ2MEeyInc_PVJ5xIQ"/>
  <Requirements:Requirement xmi:id="_TCbmEJ2NEeyInc_PVJ5xIQ" base_NamedElement="_TCZw4J2NEeyInc_PVJ5xIQ" id="REQ-TrackStatus-11" text="The L3 Trackside shall manage the overlap of multiple Unknown Track Status Areas and only remove them when it is safe to do so." base_Class="_TCZw4J2NEeyInc_PVJ5xIQ"/>
  <Requirements:Requirement xmi:id="_hA5FAJ2NEeyInc_PVJ5xIQ" base_NamedElement="_hA2owJ2NEeyInc_PVJ5xIQ" id="REQ-TrackStatus-18" text="When the L3 Trackside considers the communication session with a train is terminated, the L3 Trackside shall change the Track Status Area associated with this train from Occupied to Unknown, except if the train is completely located inside an Active Shunting Area." base_Class="_hA2owJ2NEeyInc_PVJ5xIQ"/>
  <Requirements:Requirement xmi:id="_klZtkJ2NEeyInc_PVJ5xIQ" base_NamedElement="_klVcIJ2NEeyInc_PVJ5xIQ" id="REQ-TrackStatus-19" text="When the L3 Trackside considers the communication session is terminated with a train completely located inside an Active Shunting Area, then the L3 Trackside shall remove the Occupied area of track associated with this train." base_Class="_klVcIJ2NEeyInc_PVJ5xIQ"/>
  <Requirements:Requirement xmi:id="_NeLtIJ67EeyRVZ3rwv6m0A" base_NamedElement="_NdtzEJ67EeyRVZ3rwv6m0A" id="REQ-Reserved-2" text="Following a request from the TMS, the L3 Trackside shall be able to reduce or remove an existing Reserved Status Area of track only after adjusting any given authorisation accordingly." base_Class="_NdtzEJ67EeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_JcpU4J68EeyRVZ3rwv6m0A" base_NamedElement="_JcjOQJ68EeyRVZ3rwv6m0A" base_Class="_JcjOQJ68EeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_MR-jQJ68EeyRVZ3rwv6m0A" base_NamedElement="_MR31kJ68EeyRVZ3rwv6m0A" id="REQ-FVB-1" text="For a system using Fixed Virtual Blocks, the L3 Trackside shall determine the Status of all the Fixed Virtual Blocks within the Area of Control by applying the following algorithm:&#xD;&#xA;&#x9;If there is any part of the Consolidated Track Status which is Occupied within the FVB: &#xD;&#xA;&#x9;Then&#xD;&#xA;&#x9;&#x9;The FVB has Status Occupied &#xD;&#xA;&#x9;Elseif there is any part of the Consolidated Track Status which is Unknown within the FVB &#xD;&#xA;&#x9;&#x9;the FVB has Status Unknown&#xD;&#xA;&#x9;Else&#xD;&#xA;&#x9;&#x9;the FVB has Status Clear&#xD;&#xA;&#x9;Endif" base_Class="_MR31kJ68EeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_Eej_YJ6-EeyRVZ3rwv6m0A" base_NamedElement="_EegVAJ6-EeyRVZ3rwv6m0A" id="REQ-TTD-4" text="When using TTD to clear Occupied parts of Track Status, the L3 Trackside shall ensure the resulting Occupied Track Status Area is not shorter than L_TRAIN for the train located in this area of track." base_Class="_EegVAJ6-EeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_J0XmcJ6-EeyRVZ3rwv6m0A" base_NamedElement="_J0UjIJ6-EeyRVZ3rwv6m0A" id="REQ-TTD-6" text="The L3 Trackside shall be able to remove an Unknown Track Status Area caused by a faulty TTD, if it can be sure that no train is located over it and the TMS has authorised the operation." base_Class="_J0UjIJ6-EeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_lECJ0J7LEeyRVZ3rwv6m0A" base_NamedElement="_lD_tkJ7LEeyRVZ3rwv6m0A" id="REQ-MA-1" text="The L3 Trackside shall only issue Movement Authorities to trains which have a known location." base_Class="_lD_tkJ7LEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_pdA8QJ7LEeyRVZ3rwv6m0A" base_NamedElement="_pc948J7LEeyRVZ3rwv6m0A" id="REQ-MA-2" text="The L3 Trackside shall be able to issue a Movement Authority without a mode profile for a train up to the next obstruction." base_Class="_pc948J7LEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_sh0xEJ7LEeyRVZ3rwv6m0A" base_NamedElement="_shxtwJ7LEeyRVZ3rwv6m0A" id="REQ-MA-3" text="When providing a Movement Authority without a mode profile to a train following another train, the L3 Trackside shall use the following rules for the Movement Authority:&#xD;&#xA;1) Danger Point should should be at or in rear of the CRE of the preceding train&#xD;&#xA;2) There should be at least the distance of the L3 Margin between the EoA and the CRE" base_Class="_shxtwJ7LEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_s9dkMJ7LEeyRVZ3rwv6m0A" base_NamedElement="_s9c9IJ7LEeyRVZ3rwv6m0A" id="REQ-StartTrain-2" text="The L3 Trackside shall alert the TMS of a train that, in the Start of Mission position report, is reporting an invalid or unknown position, or a valid position from an NID_BG not known to the L3 Trackside." base_Class="_s9c9IJ7LEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_tkXMIJ7LEeyRVZ3rwv6m0A" base_NamedElement="_tkWlEJ7LEeyRVZ3rwv6m0A" id="REQ-MA-4" text="When providing a Movement Authority without a mode profile for a train up to the boundary of an Unknown area, the L3 Trackside shall use the following rules for the Movement Authority:&#xD;&#xA;1) Danger Point should Point should be at or in rear of the CRE of the preceding train&#xD;&#xA;2) There should be at least the distance of the L3 Margin between the EoA and the CRE" base_Class="_tkWlEJ7LEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_uW3nIJ7LEeyRVZ3rwv6m0A" base_NamedElement="_uW2ZAJ7LEeyRVZ3rwv6m0A" id="REQ-MA-7" text="The L3 Trackside shall be able to issue a FS Movement Authority for a train to follow another train along the same path." base_Class="_uW2ZAJ7LEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_uW9twJ7LEeyRVZ3rwv6m0A" base_NamedElement="_uW2ZAZ7LEeyRVZ3rwv6m0A" id="REQ-StartTrain-3" text="The L3 Trackside shall accept from the TMS a position assigned by the Dispatcher for a train which is reporting an invalid or unknown position, if this position lies within an existing Unknown Track Status Area." base_Class="_uW2ZAZ7LEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_uW-U0J7LEeyRVZ3rwv6m0A" base_NamedElement="_uW3AEJ7LEeyRVZ3rwv6m0A" id="REQ-MA-8" text="For a system using Trackside Train Detection, the L3 Trackside shall allow multiple trains to enter in the same Trackside Train Detection section." base_Class="_uW3AEJ7LEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_ud41IJ7LEeyRVZ3rwv6m0A" base_NamedElement="_ud1x0J7LEeyRVZ3rwv6m0A" id="REQ-MA-9" text="The L3 Trackside shall be configurable to only issue Movement Authority updates relevant for operation of the railway." base_Class="_ud1x0J7LEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_ueEbUJ7LEeyRVZ3rwv6m0A" base_NamedElement="_ud1x0Z7LEeyRVZ3rwv6m0A" id="REQ-StartTrain-5" text="When accepting a position assigned for a train from the TMS, the L3 Trackside shall associate the train with the Unknown Track Status Area in which the train is positioned." base_Class="_ud1x0Z7LEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_ueFCYJ7LEeyRVZ3rwv6m0A" base_NamedElement="_ud3nAJ7LEeyRVZ3rwv6m0A" id="REQ-MA-10" text="The L3 Trackside shall report the Movement Authorities issued to trains to the Traffic Management System." base_Class="_ud3nAJ7LEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_ukpkcJ7LEeyRVZ3rwv6m0A" base_NamedElement="_ukoWUJ7LEeyRVZ3rwv6m0A" id="REQ-MA-11" text="The L3 Trackside shall include Linking Information in Movement Authorities issued to trains." base_Class="_ukoWUJ7LEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_ukvrEJ7LEeyRVZ3rwv6m0A" base_NamedElement="_ukoWUZ7LEeyRVZ3rwv6m0A" id="REQ-StartTrain-6" text="The L3 Trackside shall compare the new train data and train location information reported by a train performing Start of Mission against the information stored for the same location for a previous train." base_Class="_ukoWUZ7LEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_ukwSIJ7LEeyRVZ3rwv6m0A" base_NamedElement="_uko9YJ7LEeyRVZ3rwv6m0A" id="REQ-StartTrain-1" text="The L3 Trackside shall always accept a train during Start of Mission." base_Class="_uko9YJ7LEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_e39MoJ7MEeyRVZ3rwv6m0A" base_NamedElement="_e36wYJ7MEeyRVZ3rwv6m0A" id="REQ-EoAExclusionArea" base_Class="_e36wYJ7MEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_iIjasJ7MEeyRVZ3rwv6m0A" base_NamedElement="_iIhlgJ7MEeyRVZ3rwv6m0A" id="REQ-EoAExclusionArea-1" text="The L3 Trackside shall use End of Authority Exclusion Areas defined in configuration data." base_Class="_iIhlgJ7MEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_iIvn8J7MEeyRVZ3rwv6m0A" base_NamedElement="_iIhlgZ7MEeyRVZ3rwv6m0A" id="REQ-StartTrain-4" text="The L3 Trackside shall alert the Dispatcher via the TMS about an approximate position assigned by the Dispatcher for a train which is reporting an invalid or unknown position, if this position lies outside an existing Unknown Track Status Area." base_Class="_iIhlgZ7MEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_iIwPAJ7MEeyRVZ3rwv6m0A" base_NamedElement="_iIiMkJ7MEeyRVZ3rwv6m0A" id="REQ-EoAExclusionArea-2" text="For a train approaching an EoA Exclusion Area, the L3 Trackside shall only issue an MA such that the train can proceed beyond the EoA Exclusion Area and stop with its rear end clear of the area, taking into account the length of the train and the defined L3 Margin distance." base_Class="_iIiMkJ7MEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_5nOEkJ7MEeyRVZ3rwv6m0A" base_NamedElement="_5nKaMJ7MEeyRVZ3rwv6m0A" id="REQ-StartTrain" base_Class="_5nKaMJ7MEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_AZdNYJ7NEeyRVZ3rwv6m0A" base_NamedElement="_AZb_QJ7NEeyRVZ3rwv6m0A" id="REQ-StartTrain-7" text="If the L3 Trackside determines that the train location information from a train after performing Start of Mission does not match the stored information associated with the Unknown Track Status Area in which the train is located, then the L3 Trackside shall remove the Unknown corresponding to the area from the Min Safe Front End to the Max Safe Rear End of that train." base_Class="_AZb_QJ7NEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_AZiF4J7NEeyRVZ3rwv6m0A" base_NamedElement="_AZb_QZ7NEeyRVZ3rwv6m0A" id="REQ-StartTrain-8" text="If a train, which has not yet sent Validated Train data, reports a position with both the Min Safe Front End and the Max Safe Front End in an area of track considered Clear, then the L3 Trackside shall create an Unknown Track Status Area for the front end of this train from the reported Min Safe Front End to Max Safe Front End." base_Class="_AZb_QZ7NEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_AZis8J7NEeyRVZ3rwv6m0A" base_NamedElement="_AZb_Qp7NEeyRVZ3rwv6m0A" id="REQ-StartTrain-9" text="If a train, which has not yet sent Validated Train data, reports a position with a Confidence Interval which is partly in an Unknown Track Status Area and the Estimated Front End outside this Unknown Track Status Area, then the L3 Trackside shall extend this Unknown Track Status Area to the boundary of the reported Confidence Interval for the Estimated Front End of this train." base_Class="_AZb_Qp7NEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_AZis8Z7NEeyRVZ3rwv6m0A" base_NamedElement="_AZcmUJ7NEeyRVZ3rwv6m0A" id="REQ-StartTrain-10" text="When receiving Validated Train Data from a train which is not associated with an Unknown Track Status Area except for the Estimated Front End of this train, then the L3 Trackside shall extend this Unknown Track Status Area with the reported Train Length to the Min Safe Rear End." base_Class="_AZcmUJ7NEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_AZj7EJ7NEeyRVZ3rwv6m0A" base_NamedElement="_AZcmUZ7NEeyRVZ3rwv6m0A" id="REQ-StartTrain-11" text="If the L3 Trackside receives Validated Train Data from a train with a position within an Unknown Track Status Area for which the stored train length is less than what was reported by this train, then the L3 Trackside shall alert the TMS to the situation." base_Class="_AZcmUZ7NEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_AZkiIJ7NEeyRVZ3rwv6m0A" base_NamedElement="_AZcmUp7NEeyRVZ3rwv6m0A" id="REQ-StartTrain-12" text="The L3 Trackside shall maintain the communication session with a train reporting an LRBG at Start of Mission with an ID set to ‘unknown’ or an ID which is not known to the L3 Trackside, unless requested by the TMS to terminate the session with this train." base_Class="_AZcmUp7NEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_9p8dsJ7NEeyRVZ3rwv6m0A" base_NamedElement="_9p6BcJ7NEeyRVZ3rwv6m0A" id="REQ-StartTrain-14" text="The L3 Trackside shall, if configured, alert the TMS of a train which terminated its communication session without sending Validated Train Data to the L3 Trackside." base_Class="_9p6BcJ7NEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_-KKO0J7NEeyRVZ3rwv6m0A" base_NamedElement="_-KJAsJ7NEeyRVZ3rwv6m0A" id="REQ-StartTrain-13" text="On request from the TMS, the L3 Trackside shall order the train to terminate the communication session." base_Class="_-KJAsJ7NEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="__QrK8Z7NEeyRVZ3rwv6m0A" base_NamedElement="__Qqj4J7NEeyRVZ3rwv6m0A" id="REQ-StartTrain-15" text="The L3 Trackside shall, if configured, alert the TMS about communicating trains for which the L3 Trackside has not received Validated Train Data after a defined timeout." base_Class="__Qqj4J7NEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="__QwqgJ7NEeyRVZ3rwv6m0A" base_NamedElement="__QrK8J7NEeyRVZ3rwv6m0A" id="REQ-RadioHole-1" text="On request from the TMS, the L3 Trackside shall activate or deactivate predefined Temporary Radio Holes." base_Class="__QrK8J7NEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="__5DZkJ7NEeyRVZ3rwv6m0A" base_NamedElement="__5CLcJ7NEeyRVZ3rwv6m0A" id="REQ-RadioHole-2" text="The L3 Trackside shall establish an End of Authority Exclusion Area for each Radio Hole." base_Class="__5CLcJ7NEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="__5ISEJ7NEeyRVZ3rwv6m0A" base_NamedElement="__5CLcZ7NEeyRVZ3rwv6m0A" id="REQ-RadioHole-3" text="The L3 Trackside shall remove the End of Authority Exclusion Area created for a Temporary Radio Hole if the Radio Hole is deactivated." base_Class="__5CLcZ7NEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="__5I5IJ7NEeyRVZ3rwv6m0A" base_NamedElement="__5CygJ7NEeyRVZ3rwv6m0A" id="REQ-RadioHole-4" text="The L3 Trackside shall start the Radio Hole timer when a train enters a Radio Hole." base_Class="__5CygJ7NEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="__5JgMJ7NEeyRVZ3rwv6m0A" base_NamedElement="__5CygZ7NEeyRVZ3rwv6m0A" id="REQ-RadioHole-5" text="While the L3 Trackside considers that a train is inside a Radio Hole, the L3 Trackside shall stop supervising the following timers: Mute timer, Integrity Wait Timer, ETCS session timer" base_Class="__5CygZ7NEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_AavKUZ7OEeyRVZ3rwv6m0A" base_NamedElement="_AatVIJ7OEeyRVZ3rwv6m0A" id="REQ-RadioHole-6" text="Upon expiry of the Radio Hole timer, the L3 Trackside shall treat the train the same as for loss of communications." base_Class="_AatVIJ7OEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_Aa0p4J7OEeyRVZ3rwv6m0A" base_NamedElement="_Aat8MJ7OEeyRVZ3rwv6m0A" id="REQ-RadioHole-7" text="Upon expiry of the Radio Hole timer, the L3 Trackside shall inform the TMS that a train has not emerged from a Radio Hole." base_Class="_Aat8MJ7OEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_Aa1Q8J7OEeyRVZ3rwv6m0A" base_NamedElement="_Aat8MZ7OEeyRVZ3rwv6m0A" id="REQ-RadioHole-8" text="The L3 Trackside shall stop the Radio Hole timer when a train has left a Radio Hole." base_Class="_Aat8MZ7OEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_Aa1Q8Z7OEeyRVZ3rwv6m0A" base_NamedElement="_Aat8Mp7OEeyRVZ3rwv6m0A" id="REQ-Rev-1" text="When a train has been given Reversing Area information, the L3 Trackside shall prevent authorising other trains into the area where the train may reverse." base_Class="_Aat8Mp7OEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_Aa14AJ7OEeyRVZ3rwv6m0A" base_NamedElement="_AaujQJ7OEeyRVZ3rwv6m0A" id="REQ-Rev-2" text="The L3 Trackside shall only extend the permission to reverse (by increasing the Reversing Distance) when the extension is already covered by the Reserved Status Area in rear of the train." base_Class="_AaujQJ7OEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_Aa2fEJ7OEeyRVZ3rwv6m0A" base_NamedElement="_AaujQZ7OEeyRVZ3rwv6m0A" id="REQ-Rev-3" text="When a train has proceeded beyond the Reversing Area, the L3 Trackside shall release the Reserved Status Area behind the train." base_Class="_AaujQZ7OEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_Aa3GIJ7OEeyRVZ3rwv6m0A" base_NamedElement="_AaujQp7OEeyRVZ3rwv6m0A" id="REQ-Rev-4" text="When receiving a position report from a train in RV mode, the L3 Trackside shall change the Track Status to Unknown for the area from the Max Safe Front End to the Boundary for Reversing." base_Class="_AaujQp7OEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_Aa3GIZ7OEeyRVZ3rwv6m0A" base_NamedElement="_AavKUJ7OEeyRVZ3rwv6m0A" id="REQ-Rev-5" text="When the mute timer expires for a train which has received Reversing Area Information, has reported beyond the Boundary for Reversing but not reported with its Estimated Front End beyond the Reversing Area, then the L3 Trackside shall change the Track Status to Unknown for the location associated with the train and extend this area until to the Boundary for Reversing." base_Class="_AavKUJ7OEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_DYKHQJ7aEeyRVZ3rwv6m0A" base_NamedElement="_DYISEJ7aEeyRVZ3rwv6m0A" base_Class="_DYISEJ7aEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_CCNykJ7bEeyRVZ3rwv6m0A" base_NamedElement="_CCLWUJ7bEeyRVZ3rwv6m0A" base_Class="_CCLWUJ7bEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_D8XyIJ7bEeyRVZ3rwv6m0A" base_NamedElement="_D8VV4J7bEeyRVZ3rwv6m0A" base_Class="_D8VV4J7bEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_8RMSUJ7eEeyRVZ3rwv6m0A" base_NamedElement="_8RJ2EJ7eEeyRVZ3rwv6m0A" id="REQ-EoM-1" text="The L3 Trackside shall update the stored information of the train performing the EoM." base_Class="_8RJ2EJ7eEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_-GgOIJ7eEeyRVZ3rwv6m0A" base_NamedElement="_-GeY8J7eEeyRVZ3rwv6m0A" id="REQ-EoM-2" text="When receiving an End of Mission message from a train which is completely located inside an Active Shunting Area, then the L3 Trackside shall remove the Track Status Area associated with this train." base_Class="_-GeY8J7eEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_-VMxcJ7eEeyRVZ3rwv6m0A" base_NamedElement="_-VLjUJ7eEeyRVZ3rwv6m0A" id="REQ-EoM-3" text="When the L3 Trackside receives an End of Mission message, then the L3 Trackside shall remove any associated Reserved Status Area associated with this train." base_Class="_-VLjUJ7eEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_-ZSAMJ7eEeyRVZ3rwv6m0A" base_NamedElement="_-ZRZIJ7eEeyRVZ3rwv6m0A" id="REQ-EoM-4" text="The L3 Trackside shall be able to cope with differences in the confidence interval provided in the position report of a train that reported End of Mission even when related to the same train position." base_Class="_-ZRZIJ7eEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_UC-FcJ7fEeyRVZ3rwv6m0A" base_NamedElement="_UC8QQJ7fEeyRVZ3rwv6m0A" base_Class="_UC8QQJ7fEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_YIvoAJ7fEeyRVZ3rwv6m0A" base_NamedElement="_YIrWkJ7fEeyRVZ3rwv6m0A" id="REQ-HO" base_Class="_YIrWkJ7fEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_siM0cJ7fEeyRVZ3rwv6m0A" base_NamedElement="_siKYMJ7fEeyRVZ3rwv6m0A" base_Class="_siKYMJ7fEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_uo-TsJ7fEeyRVZ3rwv6m0A" base_NamedElement="_uo8egJ7fEeyRVZ3rwv6m0A" base_Class="_uo8egJ7fEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_xIcm0J7fEeyRVZ3rwv6m0A" base_NamedElement="_xIZjgJ7fEeyRVZ3rwv6m0A" id="REQ-Split" base_Class="_xIZjgJ7fEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_7toc0J7fEeyRVZ3rwv6m0A" base_NamedElement="_7tmAkJ7fEeyRVZ3rwv6m0A" id="REQ-LevelTrans-1" text="The L3 Trackside shall have means to detect non-communicating trains about to enter the L3 area." base_Class="_7tmAkJ7fEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_8R61oJ7fEeyRVZ3rwv6m0A" base_NamedElement="_8R6OkJ7fEeyRVZ3rwv6m0A" id="REQ-LevelTrans-2" text="For a train that has been authorised to leave the L3 Area, the L3 Trackside shall maintain the communication session with a train reporting a position with an LRBG which is not known to the L3 Trackside, while the CRE of this train is still located in the L3 Area, or until the L3 Trackside decides that it is suitable to terminate the session with this train, or until requested by the TMS to terminate the session with this train." base_Class="_8R6OkJ7fEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_8uxJsJ7fEeyRVZ3rwv6m0A" base_NamedElement="_8umxoJ7fEeyRVZ3rwv6m0A" id="REQ-HO-1" text="When acting as an Accepting L3 Trackside, the L3 Trackside shall consider the area sent as part of a Route Related Information message to an adjacent L3 Trackside as a Reserved Status Area." base_Class="_8umxoJ7fEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_8uxwwJ7fEeyRVZ3rwv6m0A" base_NamedElement="_8umxoZ7fEeyRVZ3rwv6m0A" id="REQ-HO-2" text="When the Handing Over L3 Trackside receives a position report and detects that the Confirmed Rear End of the train has crossed the border, it shall send a session termination order to the ETCS On-Board equipment." base_Class="_8umxoZ7fEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_9l3Jw57fEeyRVZ3rwv6m0A" base_NamedElement="_9l2isJ7fEeyRVZ3rwv6m0A" id="REQ-HO-3" text="If the L3 Trackside considers communication session is terminated with a train approaching the L3 Area, then the L3 Trackside shall set the Track Status to Unknown for any area that is Reserved for this train, unless this area is already Unknown." base_Class="_9l2isJ7fEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_9l8CQJ7fEeyRVZ3rwv6m0A" base_NamedElement="_9l3JwJ7fEeyRVZ3rwv6m0A" id="REQ-SH-1" text="The L3 Trackside shall be configurable with predefined Permanent and Temporary Shunting Areas." base_Class="_9l3JwJ7fEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_9l8CQZ7fEeyRVZ3rwv6m0A" base_NamedElement="_9l3JwZ7fEeyRVZ3rwv6m0A" id="REQ-SH-2" text="On request from the TMS, the L3 Trackside shall be able to activate and deactivate a Temporary Shunting Area." base_Class="_9l3JwZ7fEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_9l8pUJ7fEeyRVZ3rwv6m0A" base_NamedElement="_9l3Jwp7fEeyRVZ3rwv6m0A" id="REQ-SH-3" text="The L3 Trackside shall consider the Track Status of an Active Shunting Area to be Unknown and Non-Sweepable." base_Class="_9l3Jwp7fEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_9uW-cJ7fEeyRVZ3rwv6m0A" base_NamedElement="_9uVwUJ7fEeyRVZ3rwv6m0A" id="REQ-SH-4" text="The L3 Trackside shall perform specific checks before activating a Temporary Shunting Area." base_Class="_9uVwUJ7fEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_9ue6QJ7fEeyRVZ3rwv6m0A" base_NamedElement="_9uVwUZ7fEeyRVZ3rwv6m0A" id="REQ-Join-1" text="When there is Joining without End of Mission, the L3 Trackside shall remove the Unknown Track Status Area corresponding to the Train to be Joined and the Joining Train if the Joined Train meets the following conditions:&#xD;&#xA;a) The Joined Train has a new L_TRAIN which is equal to the sum of the L_TRAIN for the Train to be Joined and the L_TRAIN for the Joining Train, plus or minus a tolerance&#xD;&#xA;b) The Joined Train has confirmed Train Integrity." base_Class="_9uVwUZ7fEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_9ufhUJ7fEeyRVZ3rwv6m0A" base_NamedElement="_9uWXYJ7fEeyRVZ3rwv6m0A" id="REQ-Join-2" text="If the Joined Train leaves the Unknown Track Status Area, and the Joined Train has L_TRAIN less than the sum of the L_TRAIN of the Train to be Joined and the L_TRAIN of the Joining Train, then the L3 Trackside shall reduce the length of the Unknown Track Status Area by L_TRAIN of the Joined Train." base_Class="_9uWXYJ7fEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_9ugIYJ7fEeyRVZ3rwv6m0A" base_NamedElement="_9uWXYZ7fEeyRVZ3rwv6m0A" id="REQ-Join-3" text="If the Joined Train leaves the Unknown Track Status Area, and the Joined Train has L_TRAIN less than the sum of the L_TRAIN of the Train to be Joined and the L_TRAIN of the Joining Train, then the L3 Trackside shall subtract the Length of the Joined Train from the length stored for the remaining Unknown Track Status Area." base_Class="_9uWXYZ7fEeyRVZ3rwv6m0A"/>
  <Requirements:Requirement xmi:id="_93kk0p7fEeyRVZ3rwv6m0A" base_NamedElement="_93j9wJ7fEeyRVZ3rwv6m0A" id="REQ-Split-1" text="The L3 Trackside shall be able to reduce the length of the Unknown Track Status Area due to loss of Train Integrity or reception of new Validated Train Data when a train confirms integrity reporting within the Unknown Track Status Area." base_Class="_93j9wJ7fEeyRVZ3rwv6m0A"/>
</xmi:XMI>
