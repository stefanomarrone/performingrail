  <Requirements:Requirement xmi:id="_zitQsD1hEeyAv_UCn311nQ" base_NamedElement="_zihDcD1hEeyAv_UCn311nQ" id="REQ-LossTI" base_Class="_zihDcD1hEeyAv_UCn311nQ"/>
  <Requirements:Requirement xmi:id="_c3sb4D1iEeyAv_UCn311nQ" base_NamedElement="_c3m8UD1iEeyAv_UCn311nQ" id="REQ-LossTI-1" text="When receiving a position report from a train with the information 'Train integrity lost', the L3 Trackside shall change the Track Status Area associated with this train to Unknown." base_Class="_c3m8UD1iEeyAv_UCn311nQ"/>
  <Requirements:Requirement xmi:id="_c3yigD1iEeyAv_UCn311nQ" base_NamedElement="_c3njYD1iEeyAv_UCn311nQ" id="REQ-LossTI-2" text="When the L3 Trackside considers that the integrity is lost for a train, the L3 Trackside shall change the Track Status Area associated with this train to Unknown." base_Class="_c3njYD1iEeyAv_UCn311nQ"/>
  <Requirements:Requirement xmi:id="_c3zJkD1iEeyAv_UCn311nQ" base_NamedElement="_c3oKcD1iEeyAv_UCn311nQ" id="REQ-LossTI-3" text="When the L3 Trackside considers that the Train Irentegrity is lost for a train, the L3 Trackside shall react as configured." base_Class="_c3oKcD1iEeyAv_UCn311nQ"/>
  <Requirements:Requirement xmi:id="_c30XsD1iEeyAv_UCn311nQ" base_NamedElement="_c3oKcT1iEeyAv_UCn311nQ" id="REQ-LossTI-4" text="The L3 Trackside shall consider the Train Integrity as lost when 'No train integrity information' is reported longer than a configurable time (Integrity Wait Timer)." base_Class="_c3oKcT1iEeyAv_UCn311nQ"/>
  <Requirements:Requirement xmi:id="_c30-wD1iEeyAv_UCn311nQ" base_NamedElement="_c3oxgD1iEeyAv_UCn311nQ" id="REQ-LossTI-5" text="When receiving a message from a train with the information 'Train integrity confirmed by external device', the L3 Trackside shall start/restart the Integrity Wait Timer." base_Class="_c3oxgD1iEeyAv_UCn311nQ"/>
  <Requirements:Requirement xmi:id="_c32M4D1iEeyAv_UCn311nQ" base_NamedElement="_c3pYkD1iEeyAv_UCn311nQ" id="REQ-LossTI-6" text="When receiving a message from a train with the information 'Train integrity confirmed by Driver', if the L3 Trackside is configured to accept confirmation by Driver and the Integrity Wait Timer is running, then the L3 Trackside shall stop the Integrity Wait Timer." base_Class="_c3pYkD1iEeyAv_UCn311nQ"/>
  <Requirements:Requirement xmi:id="_c32z8D1iEeyAv_UCn311nQ" base_NamedElement="_c3p_oD1iEeyAv_UCn311nQ" id="REQ-LossTI-7" text="After a loss of integrity, the driver shall be made aware of the situation via an indication in the cab." base_Class="_c3p_oD1iEeyAv_UCn311nQ"/>
  <Requirements:Requirement xmi:id="_c34CED1iEeyAv_UCn311nQ" base_NamedElement="_c3qmsD1iEeyAv_UCn311nQ" id="REQ-LossTI-8" text="The L3 Trackside shall be able to be configured whether to accept Train Integrity confirmation by the driver." base_Class="_c3qmsD1iEeyAv_UCn311nQ"/>
  <Requirements:Requirement xmi:id="_c35QMD1iEeyAv_UCn311nQ" base_NamedElement="_c3rNwD1iEeyAv_UCn311nQ" id="REQ-LossTI-9" text="The L3 Trackside shall be configurable as to whether it authorises a Movement Authority for a train that has lost Integrity." base_Class="_c3rNwD1iEeyAv_UCn311nQ"/>
  <Requirements:Requirement xmi:id="_c36eUD1iEeyAv_UCn311nQ" base_NamedElement="_c3r00D1iEeyAv_UCn311nQ" id="REQ-LossTI-10" text="If the L3 Trackside receives Validated Train Data for a train with a train length different from previously reported within the same communication session, then the L3 Trackside shall consider the train as having lost Integrity." base_Class="_c3r00D1iEeyAv_UCn311nQ"/>
</xmi:XMI>
